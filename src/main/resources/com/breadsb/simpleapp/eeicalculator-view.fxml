<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" maxHeight="500.0" maxWidth="500.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17.0.2-ea" fx:controller="com.breadsb.simpleapp.EEICalculatorController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Pane prefHeight="53.0">
        <Label alignment="CENTER" layoutX="5.0" layoutY="15.0" prefHeight="20.0" text="Sprawność nominalna" textAlignment="CENTER" />
        <TextField fx:id="nominalefficiencyfield" layoutX="126.0" layoutY="12.0" prefHeight="25.0" />
    </Pane>
    <Pane prefHeight="53.0">
        <Label alignment="CENTER" layoutY="15.0" prefHeight="20.0" text="Współczynnik biomasy" textAlignment="CENTER" />
        <TextField fx:id="biomasscoefficientfield" layoutX="126.0" layoutY="12.0" prefHeight="25.0" />
    </Pane>
    <Pane prefHeight="53.0">
        <Label alignment="CENTER" layoutX="30.0" layoutY="15.0" prefHeight="20.0" text="Parametr F2" textAlignment="CENTER" />
        <TextField fx:id="parameterF2Field" layoutX="126.0" layoutY="12.0" prefHeight="25.0" />
    </Pane>
    <Button onAction="#onCalculateButtonClick" text="Calculate" />
    <Pane prefHeight="30.0">
        <Label layoutX="20.0" layoutY="5.0" text="Współczynnik EEI: " />
        <Label fx:id="eeitext" layoutX="140.0" layoutY="5.0" prefHeight="17.0" />
    </Pane>
</VBox>
